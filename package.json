{
    "name": "@inboundasia/vuetify-snackbar",
    "version": "1.0.1",
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build --target lib src/main.js",
        "lint": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
        "dev": "NODE_ENV=production vue-cli-service build --watch --target lib src/main.js"
    },
    "main": "dist/vuetify-snackbar.umd.js",
    "dependencies": {
        "babel-eslint": "^10.1.0",
        "core-js": "^3.6.5",
        "vue": "^2.6.11",
        "vuetify": "^2.6.14"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "~4.5.0",
        "@vue/cli-service": "~4.5.0",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier-vue": "^3.1.0",
        "eslint-plugin-vue": "^7.16.0",
        "lint-staged": "^9.5.0",
        "prettier": "^1.19.1",
        "sass": "~1.32.12",
        "sass-loader": "^10.4.1",
        "vue-template-compiler": "^2.6.11"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not dead"
    ],
    "gitHooks": {
        "pre-commit": "lint-staged"
    },
    "lint-staged": {
        "*.{js,jsx,vue}": [
            "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
            "git add"
        ]
    }
}
